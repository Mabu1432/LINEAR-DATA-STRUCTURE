<script>
function Printpairs(arr, n, sum) {
let Count = 0;
for (let i =0; i<n; i++)
    for (let j=i+1; j<n; j++)
        if (arr[i] + arr[j] == sum)
            document.write("(" + arr [i] + " , " + arr[j] + ")" + "<br>");
 }
 let arr = [1, 5, 7, -1, 5];
 let n = arr.length;
 let sum = 6;
 printPairs(arr, n, sum);
 </script>
